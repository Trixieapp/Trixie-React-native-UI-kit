{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "react-native"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "settings": {
      "import/resolver": {
          "node": {
              "extensions": [".js", ".android.js", ".ios.js"]
          }
      }
  }
}
